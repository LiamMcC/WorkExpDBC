<% include ../partials/header %>

<div class="row">
        
         
          <h1>Welcome to the comments page</h1>
            <br>
            
        
          <div class="grid-container">
        <% contact.forEach(function(contact) { %>
                       
                      
                        <div class="item">     
                        <h1 class="itemtitle">Id: <%= contact.id %> </h1>
                        <h1 class="itemp">Comment: <%= contact.Comment %></h1>
                        <p class="itemp">Name: <%= contact.name %></p>
                        <p class="itemp">Email: <%= contact.email %></p>
                        </div>
           
    
  
  <% }); %>
        
        </div>

    </div>

<% include ../partials/footer %>
